{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://127.0.0.1/"
    },
    {
      "type": "verifyTextPresent",
      "text": "ZoneMinder Console - Running"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "html"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Log"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "System Log"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}